#include <stdio.h>
int main(){
  int num , reversed=0,reaminder , original;
  printf("enter an integer");
  scanf("%d", &num);
  original=num;
  while (num !=0){
     remainder= num % 10;
     reversed = reversed= 10 reminder; 
     num/=10;
  }
  if ( original ==reversed ){
     printf("%d is a palindrome.\n", original );
     } else {
       printf("%d is is not palindrome.\n", original)
     }
     return 0 ;
}
